{
  "name": "laravel-doctrine/acl",
  "description": "Doctrine ACL for Doctrine 2 and Laravel 5",
  "license": "MIT",
  "keywords": [
    "doctrine",
    "laravel",
    "orm",
    "data mapper",
    "database",
    "acl",
    "abilities",
    "policies",
    "permissions",
    "roles",
    "organisations"
  ],
  "authors": [
    {
      "name": "Patrick Brouwers",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "illuminate/auth": "~5.1",
    "illuminate/config": "~5.1",
    "illuminate/contracts": "~5.1",
    "laravel-doctrine/orm": "dev-develop"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "LaravelDoctrine\\ACL\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/laravel-doctrine/orm.git"
    }
  ]
}
